TextButton_2.MouseButton1Click:connect(function()
player.Character.Head:BreakJoints()
    player.Character.Head.Position = Vector3.new(0,99999999999999,0)
end)

    	if CMD == 'FE headless' then
    	    player.Character.Head:BreakJoints()
    	    player.Character.Head.Position = Vector3.new(0,99999999999999,0)
    	end
